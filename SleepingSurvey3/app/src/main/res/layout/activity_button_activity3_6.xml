<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.yg.sleepingsurvey.ButtonActivity3_2">

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="어떤 약을 오늘 복용하였습니까?"
        android:id="@+id/textView9"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="7dp"
        android:layout_marginBottom="7dp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="(처방 받은 약 &amp; 약국에서 처방 없이 산 약)"
        android:id="@+id/textView10"
        android:layout_below="@+id/textView9"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="14dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="다음"
        android:id="@+id/next_button3_6"
        android:clickable="false"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/before_button3_6"
        android:onClick="goNextPage" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="이전"
        android:id="@+id/before_button3_6"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_alignTop="@+id/next_button"
        android:layout_alignEnd="@+id/radioButton5"
        android:onClick="goBeforePage" />

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/textView10"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/next_button3_6">

        <TextView
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="이름"
            android:id="@+id/textView14"
            android:layout_row="0"
            android:layout_column="0"
            android:singleLine="false"
            android:elegantTextHeight="false"
            android:textAlignment="center" />

        <TextView
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="투약 시간"
            android:id="@+id/textView15"
            android:layout_row="0"
            android:layout_column="1"
            android:textAlignment="center"
            android:paddingLeft="2dp"
            android:layout_marginLeft="7dp" />

        <TextView
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="용량"
            android:id="@+id/textView16"
            android:layout_row="0"
            android:layout_column="2"
            android:textAlignment="center"
            android:layout_marginLeft="7dp" />

        <EditText
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:id="@+id/editText3"
            android:layout_row="1"
            android:layout_column="0"
            android:layout_marginLeft="4dp" />

        <EditText
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:id="@+id/editText4"
            android:layout_row="1"
            android:layout_marginLeft="-5dp"
            android:layout_column="1" />

        <EditText
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:id="@+id/editText5"
            android:layout_row="1"
            android:layout_column="2"
            android:layout_marginLeft="7dp" />

        <EditText
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:id="@+id/editText6"
            android:layout_row="2"
            android:layout_column="0"
            android:layout_marginLeft="4dp" />

        <EditText
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:id="@+id/editText7"
            android:layout_row="2"
            android:layout_marginLeft="-5dp"
            android:layout_column="1" />

        <EditText
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:id="@+id/editText8"
            android:layout_row="2"
            android:layout_column="2"
            android:layout_marginLeft="7dp" />

        <EditText
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:id="@+id/editText9"
            android:layout_row="3"
            android:layout_column="0"
            android:layout_marginLeft="4dp" />

        <EditText
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:id="@+id/editText10"
            android:layout_row="3"
            android:layout_marginLeft="-5dp"
            android:layout_column="1" />

        <EditText
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:id="@+id/editText11"
            android:layout_row="3"
            android:layout_column="2"
            android:layout_marginLeft="7dp" />

        <EditText
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:id="@+id/editText12"
            android:layout_row="4"
            android:layout_column="0"
            android:layout_marginLeft="4dp" />

        <EditText
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:id="@+id/editText13"
            android:layout_row="4"
            android:layout_marginLeft="-5dp"
            android:layout_column="1" />

        <EditText
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:id="@+id/editText14"
            android:layout_row="4"
            android:layout_column="2"
            android:layout_marginLeft="7dp" />
    </GridLayout>

</RelativeLayout>
